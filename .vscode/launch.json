    "version": "0.2.0",
    "configurations": [
        
        
        {
            "type": "node",
            "request": "launch",
            "name": "Jest Tests",
            "program": "${workspaceRoot}/node_modules/@berun/dev-scripts/scripts/test.js",
            "cwd": "${workspaceFolder}/packages/runner-webpack",
            "args": [
                "--i"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "start sample",
            "program": "${workspaceFolder}/packages/berun-scripts/scripts/cmd",
            "args": ["start:static"],
            "cwd": "${workspaceFolder}/packages/template-fuse-box-docs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test sample",
            "program": "${workspaceFolder}/packages/berun-scripts/scripts/cmd",
            "args": ["test"],
            "cwd": "${workspaceFolder}/packages/templtae-react"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Build sample",
            "program": "${workspaceFolder}/packages/berun-scripts/scripts/cmd",
            "args": ["build"],
            "cwd": "${workspaceFolder}/packages/template-fuse-box-docs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Build Docs",
            "program": "${workspaceFolder}/packages/berun-scripts/scripts/cmd",
            "args": ["build:static"],
            "cwd": "${workspaceFolder}/packages/template-docs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Docs",
            "program": "${workspaceFolder}/packages/berun-scripts/scripts/cmd",
            "args": ["start:static"],
            "cwd": "${workspaceFolder}/packages/template-docs"
        }
    ]
}