{
  "name": "@bestatic/core",
  "version": "3.0.9",
  "description": "React Static Documentation",
  "main": "dist/index.js",
  "ts:main": "src/index.ts",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "authors": [
    "OffGrid Networks <admin@offgridnetworks.com>"
  ],
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "files": [
    "dist",
    "preset",
    "src"
  ],
  "keywords": [
    "mdx",
    "react",
    "docs",
    "documentation",
    "react-static"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/bestyled/bestatic/tree/master/packages/bestatic-core",
  "scripts": {
    "prebuild": "berun-dev clean",
    "build": "berun-dev build",
    "coverage": "berun-dev test --coverage",
    "format": "berun-dev format",
    "lint": "berun-dev lint",
    "prepublishOnly": "yarn build",
    "test": "berun-dev test",
    "test:watch": "berun-dev test -o --watch"
  },
  "dependencies": {
    "@bestatic/components": "3.0.9",
    "is-absolute-url": "^3.0.3",
    "lodash.get": "^4.4.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@berun/dev-scripts": "3.0.9"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
