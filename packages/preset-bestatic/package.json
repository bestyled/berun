{
  "name": "@berun/preset-bestatic",
  "version": "3.0.9",
  "description": "TDX and MDX-based documentation server",
  "main": "dist/index.js",
  "ts:main": "src/index.ts",
  "types": "src/index.ts",
  "authors": [
    "OffGrid Networks <admin@offgridnetworks.com>"
  ],
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "mdx",
    "react",
    "docs",
    "documentation",
    "react-static"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "berun-dev clean",
    "build": "berun-dev build",
    "coverage": "berun-dev test --coverage",
    "format": "berun-dev format",
    "lint": "berun-dev lint",
    "prepublishOnly": "yarn build",
    "test": "berun-dev test",
    "test:watch": "berun-dev test -o --watch"
  },
  "dependencies": {
    "@berun/berun": "3.0.9",
    "react-dev-utils": "^8.0.0",
    "resolve-cwd": "^2.0.0"
  },
  "devDependencies": {
    "@berun/preset-fuse-box-react": "3.0.9",
    "@berun/preset-react": "3.0.9",
